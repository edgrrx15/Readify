{
  "expo": {
    "name": "Readify",
    "slug": "readify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.jpeg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.readify.app"
    },
    "android": {
      "package": "com.readify.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.jpeg",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "00642c1f-6e1f-48ac-97ab-c1d422e639fd"
      }
    },
    "owner": "edgrrx",
    "plugins": [
      "expo-font"
    ]
  }
}
